{
  "name": "ant-menu",
  "version": "0.0.0",
  "description": "Webcomponent ant-menu following open-wc recommendations",
  "author": "ant-menu",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook",
    "start:build": "es-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "lit-html": "^1.1.2",
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "es-dev-server": "^1.23.0",
    "@open-wc/testing-karma": "^3.0.0",
    "deepmerge": "^3.2.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/demoing-storybook": "^1.0.1",
    "@open-wc/building-rollup": "^0.21.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.4"
  }
}